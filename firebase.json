{
  "hosting": {
    "public": "dist/public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/api/**",
        "run": {
          "serviceId": "profgui",
          "region": "europe-west1"
        }
      },
      { "source": "**", "destination": "/index.html" }
    ]
  }
}
